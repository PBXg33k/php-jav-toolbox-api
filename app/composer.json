{
    "name": "pbxg33k/php-jav-toolbox",
    "description": "Manage your JAV collection easily",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "ext-json": "^1.7",
        "colinodell/symfony-lts-or-current": "dev-master",
        "doctrine/collections": "^1.5",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "enqueue/amqp-lib": "^0.9.2",
        "enqueue/enqueue-bundle": "^0.9.3",
        "enqueue/messenger-adapter": "^0.2.0",
        "facile-it/doctrine-mysql-come-back": "^1.6",
        "friendsofphp/php-cs-fixer": "^2.14",
        "mhor/php-mediainfo": "^4.1",
        "pbxg33k/jav-info": "dev-master",
        "php-coveralls/php-coveralls": "^2.1",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/amqp-pack": "^1.0",
        "symfony/console": "^4.0",
        "symfony/event-dispatcher": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/messenger": "^4.0",
        "symfony/monolog-bundle": "^3.2",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.0",
        "symfony/security-csrf": "^4.0",
        "symfony/serializer": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "2.0.x-dev",
        "roave/security-advisories": "dev-master",
        "symfony/dotenv": "^4.0",
        "symfony/maker-bundle": "^1.4",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/test-pack": "^1.0",
        "mi-schi/phpmd-extension": "^4.3",
        "pbxg33k/phpmd-code-climate-renderer": "dev-master",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan-doctrine": "^0.11.1",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "phpstan/phpstan-symfony": "^0.11.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C9V2PD8DDMYAAPG5X5PS2SNT",
            "allow-contrib": false
        }
    }
}
